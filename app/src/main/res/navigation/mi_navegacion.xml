<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/mi_navegacion"
    app:startDestination="@id/listadoFragment">

    <fragment
        android:id="@+id/listadoFragment"
        android:name="com.example.libretadecontactos.ui.fragments.ListadoFragment"
        android:label="ListadoFragment" >
        <action
            android:id="@+id/action_listadoFragment_to_addContactoFragment"
            app:destination="@id/addContactoFragment" />
        <action
            android:id="@+id/action_listadoFragment_to_infoContactoFragment"
            app:destination="@id/infoContactoFragment" />
    </fragment>
    <fragment
        android:id="@+id/addContactoFragment"
        android:name="com.example.libretadecontactos.ui.fragments.AddContactoFragment"
        android:label="AddContactoFragment" />
    <fragment
        android:id="@+id/infoContactoFragment"
        android:name="com.example.libretadecontactos.ui.fragments.InfoContactoFragment"
        android:label="InfoContactoFragment" />
</navigation>